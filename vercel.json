{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://boxai-backend.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/out/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/out/index.html"
    }
  ]
}
